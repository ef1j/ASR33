10  REM MANDELBROT SET - HP2000 BASIC
20  DIM C$[72]
30  C$="-+=*35CV49FO6D#AZUGS02$%P&WXEQH8B@KRNM "
40  X1=69
50  Y1=30
60  I1=-1.6
70  I2=1.6
80  R1=-2
90  R2=2
100  S1=(R2-R1)/X1
110  S2=(I2-I1)/Y1
120  FOR Y=0 TO Y1
130  I3=I1+S2*Y
140  FOR X=0 TO X1
150  R3=R1+S1*X
160  Z1=R3
170  Z2=I3
180  FOR N=1 TO LEN(C$)
190  A=Z1*Z1
200  B=Z2*Z2
210  IF A+B>4 THEN 260
220  Z2=2*Z1*Z2+I3
230  Z1=A-B+R3
240  NEXT N
250  N=LEN(C$)
260  PRINT C$[N,N];
270  NEXT X
280  PRINT
290  NEXT Y
300  END
